{
  "extends": "@mate-academy/students-ts-config",
  "include": [
    "src"
, "vite-env.d.ts"  ],
  "compilerOptions": {
    "sourceMap": false,
    "types": ["node"],
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
